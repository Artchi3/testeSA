{
  "store.custom#mioneto": {
    "parent": {
      "header": "header#empty",
      "footer": "footer#empty"
    },
    "blocks": [
      "responsive-layout.desktop",
      "responsive-layout.tablet",
      "responsive-layout.phone"
    ]
  },  
  "responsive-layout.desktop": {
    "children": [ 
      "flex-layout.row#mioneto"
    ]
  },
  "responsive-layout.tablet": {
    "children": [ 
      "flex-layout.row#mioneto" 
    ]
  },
  "responsive-layout.phone": {
    "children": [ 
      "flex-layout.row#mioneto",
      "flex-layout.row#mioneto-videosession"
    ]
  },
  
  "flex-layout.row#mioneto": {
    "children": [ 
      "flex-layout.col#mioneto-content"
    ]
  },

  "flex-layout.col#mioneto-content": {
    "children": [
      "flex-layout.row#mioneto-firstscroll",
      "flex-layout.row#mioneto-secondsession",
      "flex-layout.row#mioneto-thirdsession",
      "flex-layout.row#mioneto-fourthsession",
      "flex-layout.row#mioneto-fitfhsession",
      "flex-layout.row#mioneto-sixthsession" 
    ] 
  },
  "flex-layout.row#mioneto-firstscroll":{
    "children":[
      "image#bkg-paralax",
      "image#logotype",
      "rich-text#mioneto-lparagraph",
      "image#uglybtn"
    ],
   "props": {
    "blockClass":"firstscroll"
   }
  },
  "flex-layout.row#mioneto-secondsession":{
    "children": [ 
      "flex-layout.col#second-subssesion"
    ]
  },
  "flex-layout.row#mioneto-thirdsession":{
    "children": [ 
      "flex-layout.col#third-subssesion", 
      "image#thrsub-img"
    ],
    "props":{
      "verticalAlign": "middle",
      "horizontalAlign":"center",
      "blockClass":"thirdsession"
    }
  },
  "flex-layout.row#mioneto-fourthsession":{
    "children": [ 
      "image#foursub-img",
      "flex-layout.col#fourth-subssesion"
    ],
    "props":{
      "verticalAlign": "middle",
      "horizontalAlign":"left",
      "blockClass":"fourthsession"
    }
  },
  "flex-layout.row#mioneto-fitfhsession":{
    "children": [ 
      "image#fitfhsub-img",
      "flex-layout.col#fitfh-subssesion",
      "flex-layout.col#fitfh-catalog"
    ],
    "props":{
      "verticalAlign": "middle",
      "horizontalAlign":"left",
      "blockClass":"fitfhsession"
    }
  },
  "flex-layout.row#mioneto-sixthsession":{
    "children":[
      "slider-layout#sixthsub-carousel"
    ]
  },
  "flex-layout.row#mioneto-videosession":{
    "children":[
      "rich-text#video-title",
      "rich-text#video-smtxt",
      "video#mioneto"
    ],
    "props":{
      "blockClass":"videosess"
    }
  }, 
  "flex-layout.col#second-subssesion":{
    "children":[
      "rich-text#secsub-txt",
      "rich-text#secsub-title",
      "rich-text#secsub-smtxt",
      "image#secsub-img"
    ],
    "props":{
      "verticalAlign": "middle",
      "horizontalAlign":"center",
      "blockClass":"second-subssesion"
    }
  },
  "flex-layout.col#third-subssesion":{
    "children": [  
      "rich-text#thrsub-txt",
      "rich-text#thrsub-title",
      "rich-text#thrsub-smtxt"
    ],
    "props":{
      "verticalAlign": "middle",
      "horizontalAlign":"left",
      "blockClass":"thirdsession-col"
    }
  },
  "flex-layout.col#fourth-subssesion":{
    "children": [  
      "rich-text#foursub-txt",
      "rich-text#foursub-title",
      "rich-text#foursub-smtxt"
    ],
    "props":{
      "verticalAlign": "middle",
      "horizontalAlign":"left",
      "blockClass":"fourthsession-col"
    }
  },
  "flex-layout.col#fitfh-subssesion":{
    "children": [   
      "rich-text#fitfhsub-title",
      "rich-text#fitfhsub-smtxt"
    ],
    "props":{
      "verticalAlign": "middle",
      "horizontalAlign":"left",
      "blockClass":"fitfhsession-col"
    }
  },
  "flex-layout.col#fitfh-catalog":{
    "children": [   
      "flex-layout.row#catalog"
    ],
    "props":{
      "verticalAlign": "middle",
      "horizontalAlign":"left",
      "blockClass":"fitfhsession-catalog"
    }
  },
  "flex-layout.row#catalog":{ 
    "children":[
      "info-card#catalog1",
      "info-card#catalog2",
